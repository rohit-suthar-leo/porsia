# AWS Configuration for Waitlist
AWS_REGION=ap-south-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here

# S3 Bucket Name (must match what you created)
AWS_S3_BUCKET_NAME=myporsia-waitlist

# SES Email Configuration
AWS_SES_FROM_EMAIL=hello@myporsia.com
ADMIN_EMAIL=your-admin-email@example.com

# Optional: Analytics
# GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# AWS Cognito Configuration
COGNITO_USER_POOL_ID=ap-south-1_xxxxx
COGNITO_CLIENT_ID=xxxxx
COGNITO_CLIENT_SECRET=xxxxx
SESSION_SECRET=change-this-to-a-random-string-in-production
